import java.util.Scanner;
public class KRS
{
    public static void main(String[] args)
    {
        int n,i,j;
        Scanner sc = new Scanner(System.in);

        System.out.println("Enter the number of rows");
        n= sc.nextInt();

        for ( i=5;i>=1;i--)
        {
            for ( j=5;j>=i;j--)
            {
                System.out.print(j + " ");

            }
            System.out.println();
        }
        sc.close();
    }
}
